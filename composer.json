{
    "name": "webtechnick/laravel-macros",
    "description": "a useful set of collection macros for laravel",
    "type": "library",
    "require": {
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "WebTechNick\\LaravelMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebTechNick\\LaravelMacros\\Test\\": "tests"
        }
    },
     "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebTechNick\\LaravelMacros\\LaravelMacroServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Baker",
            "email": "nick@webtechnick.com"
        }
    ]
}
